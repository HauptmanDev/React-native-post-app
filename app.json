{
  "expo": {
    "name": "Self Blog",
    "slug": "rn-self-blog",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hauptmandev.self-blog-rn-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.hauptmandev.self_blog_rn_app",
      "versionCode": 1,
      "permissions": ["CAMERA", "WHITE_EXTERNAL_STORAGE"]
    }
  }
}
